{
  "name": "homepage",
  "version": "1.0.0",
  "description": "[![pages-build-deployment](https://github.com/triax/homepage/actions/workflows/pages/pages-build-deployment/badge.svg)](https://github.com/triax/homepage/actions/workflows/pages/pages-build-deployment)",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "img:download": "npx tsx scripts/download-all-images.ts",
    "img:download:force": "SKIP_EXISTING=false npx tsx scripts/download-all-images.ts",
    "img:sync": "npx tsx scripts/download-all-images.ts --sync",
    "img:sync:dry": "npx tsx scripts/download-all-images.ts --sync --dry-run",
    "img:check": "npx tsx scripts/check-image-sync.ts",
    "img:cleanup": "npx tsx scripts/cleanup-unused-images.ts",
    "img:cleanup:dry": "npx tsx scripts/cleanup-unused-images.ts",
    "img:cleanup:force": "npx tsx scripts/cleanup-unused-images.ts --force",
    "img:cleanup:interactive": "npx tsx scripts/cleanup-unused-images.ts --force --interactive",
    "img:mapping": "npx tsx scripts/create-image-mapping.ts",
    "img:check:browser": "npx tsx scripts/check-missing-images.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "otiai10",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.2.1",
    "playwright": "^1.54.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  }
}
