# Instagram API Settings
# Instagram User ID and Access Token for fetching posts
IG_USER_ID=your_instagram_user_id_here
IG_ACCESS_TOKEN=your_instagram_access_token_here

# Optional: Number of posts to fetch (default: 3)
# IG_LIMIT=3

# Facebook API Settings
# Required for exchanging short-live tokens with long-lived tokens
FACEBOOK_APP_ID=your_facebook_app_id_here
FACEBOOK_APP_SECRET=your_facebook_app_secret_here
